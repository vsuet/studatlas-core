{
  "name": "studatlas-core",
  "version": "0.0.1",
  "description": "studatlas backend",
  "repository": {
    "type": "git",
    "url": "git@github.com:pereslavtsev/studatlas-core.git"
  },
  "bugs": "https://github.com/pereslavtsev/studatlas-core/issues",
  "author": {
    "name": "Vitaly Pereslavtsev",
    "email": "dev@pereslavtsev.com",
    "url": "https://pereslavtsev.com"
  },
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.3",
    "@nestjs/cli": "^6.12.1",
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/microservices": "^6.10.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-fastify": "^6.7.2",
    "apollo-server-fastify": "^2.9.4",
    "auth0": "^2.27.1",
    "dotenv": "^8.1.0",
    "graphql": "^14.5.6",
    "graphql-tag-pluck": "^0.8.7",
    "graphql-tools": "^4.0.5",
    "grpc": "^1.24.0",
    "jwks-rsa": "^1.6.0",
    "nestjs-config": "^1.4.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/auth0": "^2.9.21",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/passport-jwt": "^3.0.2",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "^1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
